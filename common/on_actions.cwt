types = {
    type[on_action] = {
        path = "game/common/on_actions"
        start_from_root = yes
	}
}

on_action = {
	## cardinality = 0..1
	events = {
		## cardinality = 0..inf
		<event>
	}
	## cardinality = 0..1
	random_events = {
		## cardinality = 0..inf
		int = 0
		## cardinality = 0..inf
		int = <event>
	}
	alias_name[effect] = alias_match_left[effect]
}
